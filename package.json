{
  "name": "storybook-addon-angularjs-workspace",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "lib",
    "example",
    "exampleWC"
  ],
  "scripts": {
    "format:all-files": "prettier --write ."
  },
  "devDependencies": {
    "husky": "^4.3.6",
    "lerna": "^3.22.1",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md,json}": "prettier --write"
  }
}
